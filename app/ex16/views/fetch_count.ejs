<script>
    function test_temp(){ //then방식
        fetch("/get_count")
        .then(res => {
            return res.json()
        }).then(data =>{
            console.log("data:",data)
            const count = document.getElementById("count")
            count.innerHTML = data.cnt
        })
    }
    async function test(){ //async방식
        const res = await fetch("/get_count")
        console.log("res:", res)
        const data = await res.json()
        console.log("data:", data)
        const count = document.getElementById("count")
        count.innerHTML = data.cnt
    }    
</script>
<h1>일</h1><h1>이</h1><h1>삼</h1><h1>사</h1>
<h1>오</h1><h1>육</h1><h1>칠</h1><h1>팔</h1>
